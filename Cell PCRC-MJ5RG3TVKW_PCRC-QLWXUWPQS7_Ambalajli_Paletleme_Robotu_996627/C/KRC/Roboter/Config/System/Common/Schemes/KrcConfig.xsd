            <xs:element ref="MAX_UP_TRIGGER_TIMEOUT"/>
            <xs:element ref="Customer_VW"/>
            <xs:element ref="Customer_KUKA"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
