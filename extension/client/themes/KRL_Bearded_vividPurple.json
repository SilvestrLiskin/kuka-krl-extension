{
  "name": "KRL Bearded vividPurple",
  "type": "dark",
  "colors": {
    "editor.background": "#171131",
    "editor.foreground": "#a699db",
    "activityBar.background": "#130e29",
    "sideBar.background": "#130e29",
    "statusBar.background": "#171131",
    "editorLineNumber.foreground": "#c7bfe8"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5a4e8d"
      }
    },
    {
      "scope": [
        "meta.fold.krl",
        "comment.line.fold.krl"
      ],
      "settings": {
        "foreground": "#9085bc",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "comment.line.todo.krl"
      ],
      "settings": {
        "foreground": "#FF7135",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#42DD76"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#FF7135"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#D62C2C"
      }
    },
    {
      "scope": [
        "keyword.control.krl"
      ],
      "settings": {
        "foreground": "#E66DFF"
      }
    },
    {
      "scope": [
        "keyword.function.movement.krl"
      ],
      "settings": {
        "foreground": "#28A9FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#a699db"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#14E5D4"
      }
    },
    {
      "scope": [
        "variable.language.system.krl"
      ],
      "settings": {
        "foreground": "#FFB638",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "support.function"
      ],
      "settings": {
        "foreground": "#28A9FF"
      }
    },
    {
      "scope": [
        "punctuation.krl"
      ],
      "settings": {
        "foreground": "#a699db"
      }
    },
    {
      "scope": [
        "comment punctuation"
      ],
      "settings": {
        "foreground": "#5a4e8d"
      }
    }
  ]
}