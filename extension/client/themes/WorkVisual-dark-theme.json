{
    "$schema": "vscode://schemas/color-theme",
    "name": "KUKA WorkVisual Dark",
    "type": "dark",
    "colors": {
        "editor.background": "#1E1E1E",
        "editor.foreground": "#D4D4D4",
        "editor.selectionBackground": "#264F78",
        "editor.lineHighlightBackground": "#2A2D2E80",
        "editor.inactiveSelectionBackground": "#3A3D41",
        "editorCursor.foreground": "#AEAFAD",
        "editorWhitespace.foreground": "#3B3B3B",
        "editorLineNumber.foreground": "#858585",
        "editorLineNumber.activeForeground": "#C6C6C6",
        "editorIndentGuide.background1": "#404040",
        "editorIndentGuide.activeBackground1": "#707070",
        "editorBracketMatch.background": "#0D3A58",
        "editorBracketMatch.border": "#888888",
        "editor.foldBackground": "#2D4A5E40",
        "sideBar.background": "#252526",
        "sideBar.foreground": "#CCCCCC",
        "activityBar.background": "#333333",
        "activityBar.foreground": "#FFFFFF",
        "statusBar.background": "#FF6600",
        "statusBar.foreground": "#FFFFFF",
        "titleBar.activeBackground": "#3C3C3C",
        "titleBar.activeForeground": "#CCCCCC"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "comment.line",
                "comment.block",
                "comment.line.semicolon.krl",
                "comment.line.ampersand.krl"
            ],
            "settings": {
                "foreground": "#6A9955"
            }
        },
        {
            "name": "FOLD regions",
            "scope": [
                "meta.fold.krl",
                "comment.line.fold.krl"
            ],
            "settings": {
                "foreground": "#808080",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.control.krl",
                "keyword.other.krl"
            ],
            "settings": {
                "foreground": "#569CD6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Storage types",
            "scope": [
                "storage.type",
                "storage.type.krl",
                "storage.modifier",
                "storage.type.interrupt.krl",
                "storage.type.trigger.krl"
            ],
            "settings": {
                "foreground": "#569CD6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Movement commands",
            "scope": [
                "keyword.function.movement.krl"
            ],
            "settings": {
                "foreground": "#C586C0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Approximation modes",
            "scope": [
                "constant.language.approximation.krl"
            ],
            "settings": {
                "foreground": "#C586C0"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.quoted.double.krl"
            ],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.numeric.krl",
                "constant.numeric.hex.krl",
                "constant.numeric.binary.krl"
            ],
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "name": "Boolean constants",
            "scope": [
                "constant.language",
                "constant.language.boolean.krl"
            ],
            "settings": {
                "foreground": "#569CD6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Enum values",
            "scope": [
                "variable.other.enummember.krl",
                "constant.language.enum.krl"
            ],
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "System variables ($)",
            "scope": [
                "variable.language.system.krl",
                "punctuation.definition.variable.krl"
            ],
            "settings": {
                "foreground": "#4FC1FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Axis components",
            "scope": [
                "variable.language.axis.krl"
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "Data arrays",
            "scope": [
                "variable.language.data.krl"
            ],
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.function.builtin.krl"
            ],
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "keyword.operator.comparison.krl",
                "keyword.operator.arithmetic.krl",
                "keyword.operator.logical.krl",
                "keyword.operator.bitwise.krl"
            ],
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.krl",
                "punctuation.definition.enum.krl"
            ],
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "name": "Invalid identifiers",
            "scope": [
                "invalid.illegal.identifier.digit.krl",
                "invalid.illegal.identifier.toolong.krl"
            ],
            "settings": {
                "foreground": "#F44747",
                "fontStyle": "underline"
            }
        }
    ]
}