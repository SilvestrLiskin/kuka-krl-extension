{
  "name": "KRL Bearded melleJulie",
  "type": "dark",
  "colors": {
    "editor.background": "#1c1f24",
    "editor.foreground": "#b1b7c1",
    "activityBar.background": "#171a1e",
    "sideBar.background": "#171a1e",
    "statusBar.background": "#1c1f24",
    "editorLineNumber.foreground": "#cdd1d8"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6d6d6d"
      }
    },
    {
      "scope": [
        "meta.fold.krl",
        "comment.line.fold.krl"
      ],
      "settings": {
        "foreground": "#a0a0a0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "comment.line.todo.krl"
      ],
      "settings": {
        "foreground": "#edb492",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#71e893"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#edb492"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#e55454"
      }
    },
    {
      "scope": [
        "keyword.control.krl"
      ],
      "settings": {
        "foreground": "#c3a1e5"
      }
    },
    {
      "scope": [
        "keyword.function.movement.krl"
      ],
      "settings": {
        "foreground": "#63c0ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#b1b7c1"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#50dbd4"
      }
    },
    {
      "scope": [
        "variable.language.system.krl"
      ],
      "settings": {
        "foreground": "#ecd282",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "support.function"
      ],
      "settings": {
        "foreground": "#63c0ff"
      }
    },
    {
      "scope": [
        "punctuation.krl"
      ],
      "settings": {
        "foreground": "#b1b7c1"
      }
    },
    {
      "scope": [
        "comment punctuation"
      ],
      "settings": {
        "foreground": "#6d6d6d"
      }
    }
  ]
}