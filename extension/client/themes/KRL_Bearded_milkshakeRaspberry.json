{
    "name": "KRL Bearded Milkshake Raspberry",
    "type": "light",
    "colors": {
        "editor.background": "#f1e8eb",
        "editor.foreground": "#15070c",
        "editorCursor.foreground": "#0076c5",
        "editorIndentGuide.activeBackground": "#a75b72",
        "activityBar.background": "#eadde1",
        "activityBarBadge.background": "#0076c5",
        "activityBarBadge.foreground": "#f1e8eb",
        "sideBar.background": "#f1e8eb",
        "sideBar.border": "#d4b8c2",
        "list.activeSelectionForeground": "#0076c5",
        "list.activeSelectionBackground": "#f6eff1",
        "list.hoverBackground": "#eee3e6",
        "statusBar.background": "#eadde1",
        "statusBar.foreground": "#15070c",
        "tab.activeBorder": "#0076c5",
        "tab.activeBackground": "#f1e8eb",
        "titleBar.activeBackground": "#eadde1"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "punctuation.definition.comment.krl"
            ],
            "settings": {
                "foreground": "#a75b72",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.fold.krl",
                "comment.line.fold.krl"
            ],
            "settings": {
                "foreground": "#7d1f3b",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "keyword.function.movement.krl",
            "settings": {
                "foreground": "#0076c5",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.krl",
            "settings": {
                "foreground": "#7522d3"
            }
        },
        {
            "scope": "storage.type.krl",
            "settings": {
                "foreground": "#c08403",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.variable.system.krl",
            "settings": {
                "foreground": "#d12525",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#008b17"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#b96000"
            }
        },
        {
            "scope": "variable.language.data.krl",
            "settings": {
                "foreground": "#008f8f"
            }
        },
        {
            "scope": "entity.name.function.krl",
            "settings": {
                "foreground": "#c121a4"
            }
        }
    ]
}