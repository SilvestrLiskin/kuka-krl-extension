{
    "name": "KRL Bearded Monokai Stone",
    "type": "dark",
    "colors": {
        "editor.background": "#2A2D33",
        "editor.foreground": "#dee0e4",
        "editorCursor.foreground": "#78dce8",
        "editorIndentGuide.activeBackground": "#656b78",
        "activityBar.background": "#25282d",
        "activityBarBadge.background": "#78dce8",
        "activityBarBadge.foreground": "#2A2D33",
        "sideBar.background": "#2A2D33",
        "sideBar.border": "#1c1e22",
        "list.activeSelectionForeground": "#78dce8",
        "list.activeSelectionBackground": "#363941",
        "list.hoverBackground": "#282b30",
        "statusBar.background": "#25282d",
        "statusBar.foreground": "#dee0e4",
        "tab.activeBorder": "#78dce8",
        "tab.activeBackground": "#2A2D33",
        "titleBar.activeBackground": "#25282d"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "punctuation.definition.comment.krl"
            ],
            "settings": {
                "foreground": "#656b78",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.fold.krl",
                "comment.line.fold.krl"
            ],
            "settings": {
                "foreground": "#2c3e64",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "keyword.function.movement.krl",
            "settings": {
                "foreground": "#78dce8",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.krl",
            "settings": {
                "foreground": "#ab9df2"
            }
        },
        {
            "scope": "storage.type.krl",
            "settings": {
                "foreground": "#ffd866",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.variable.system.krl",
            "settings": {
                "foreground": "#fc6a67",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#a9dc76"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#fc9867"
            }
        },
        {
            "scope": "variable.language.data.krl",
            "settings": {
                "foreground": "#78e8c6"
            }
        },
        {
            "scope": "entity.name.function.krl",
            "settings": {
                "foreground": "#e991e3"
            }
        }
    ]
}