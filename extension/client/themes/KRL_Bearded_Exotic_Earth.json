{
  "name": "KRL Bearded Exotic Earth",
  "type": "dark",
  "colors": {
    "editor.background": "#221b1b",
    "editor.foreground": "#d6cccc",
    "activityBar.background": "#1c1616",
    "sideBar.background": "#1c1616",
    "statusBar.background": "#221b1b",
    "editorLineNumber.foreground": "#685454"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#685454"
      }
    },
    {
      "scope": [
        "meta.fold.krl",
        "comment.line.fold.krl"
      ],
      "settings": {
        "foreground": "#846b6b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "comment.line.todo.krl"
      ],
      "settings": {
        "foreground": "#D1711D",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#639E29"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#D1711D"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#C13838"
      }
    },
    {
      "scope": [
        "keyword.control.krl"
      ],
      "settings": {
        "foreground": "#A8547A"
      }
    },
    {
      "scope": [
        "keyword.function.movement.krl"
      ],
      "settings": {
        "foreground": "#BA9D6F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#d6cccc"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#4EAAA4"
      }
    }
  ]
}