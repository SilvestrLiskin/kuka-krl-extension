{
    "name": "KRL Bearded  H C Midnight Void",
    "type": "dark",
    "colors": {
        "editor.background": "#151f27",
        "editor.foreground": "#c3d2de",
        "editorCursor.foreground": "#7fd7f5",
        "editorIndentGuide.activeBackground": "#436179",
        "activityBar.background": "#111920",
        "activityBarBadge.background": "#7fd7f5",
        "activityBarBadge.foreground": "#151f27",
        "sideBar.background": "#151f27",
        "sideBar.border": "#0a0f13",
        "list.activeSelectionForeground": "#7fd7f5",
        "list.activeSelectionBackground": "#1e2c38",
        "list.hoverBackground": "#131c24",
        "statusBar.background": "#111920",
        "statusBar.foreground": "#c3d2de",
        "tab.activeBorder": "#7fd7f5",
        "tab.activeBackground": "#151f27",
        "titleBar.activeBackground": "#111920"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "punctuation.definition.comment.krl"
            ],
            "settings": {
                "foreground": "#436179",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.fold.krl",
                "comment.line.fold.krl"
            ],
            "settings": {
                "foreground": "#173b58",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "keyword.function.movement.krl",
            "settings": {
                "foreground": "#7fd7f5",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.krl",
            "settings": {
                "foreground": "#bc98ff"
            }
        },
        {
            "scope": "storage.type.krl",
            "settings": {
                "foreground": "#F5DF76",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.variable.system.krl",
            "settings": {
                "foreground": "#fd604f",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#AFEA7B"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#ffaa7d"
            }
        },
        {
            "scope": "variable.language.data.krl",
            "settings": {
                "foreground": "#22D3B1"
            }
        },
        {
            "scope": "entity.name.function.krl",
            "settings": {
                "foreground": "#e4a3df"
            }
        }
    ]
}