    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#796d4a",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#008b17"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#b96000"
            }
        },
        {
            "name": "KRL Comments",
            "scope": [
                "comment.line"
            ],
            "settings": {
                "foreground": "#796d4a",
                "fontStyle": "italic"
            }
        },
