{
    "name": "KRL Bearded  Chocolate Espresso",
    "type": "dark",
    "colors": {
        "editor.background": "#2e2424",
        "editor.foreground": "#d2b2b2",
        "editorCursor.foreground": "#7fd7f5",
        "editorIndentGuide.activeBackground": "#5d4242",
        "activityBar.background": "#281f1f",
        "activityBarBadge.background": "#7fd7f5",
        "activityBarBadge.foreground": "#2e2424",
        "sideBar.background": "#2e2424",
        "sideBar.border": "#1d1717",
        "list.activeSelectionForeground": "#7fd7f5",
        "list.activeSelectionBackground": "#5d423d",
        "list.hoverBackground": "#2b2222",
        "statusBar.background": "#281f1f",
        "statusBar.foreground": "#d2b2b2",
        "tab.activeBorder": "#7fd7f5",
        "tab.activeBackground": "#2e2424",
        "titleBar.activeBackground": "#281f1f"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "punctuation.definition.comment.krl"
            ],
            "settings": {
                "foreground": "#5d4242",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.fold.krl",
                "comment.line.fold.krl"
            ],
            "settings": {
                "foreground": "#3d1616",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "keyword.function.movement.krl",
            "settings": {
                "foreground": "#7fd7f5",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.krl",
            "settings": {
                "foreground": "#bc98ff"
            }
        },
        {
            "scope": "storage.type.krl",
            "settings": {
                "foreground": "#F5DF76",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.variable.system.krl",
            "settings": {
                "foreground": "#fd604f",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#AFEA7B"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#ffaa7d"
            }
        },
        {
            "scope": "variable.language.data.krl",
            "settings": {
                "foreground": "#22D3B1"
            }
        },
        {
            "scope": "entity.name.function.krl",
            "settings": {
                "foreground": "#e4a3df"
            }
        }
    ]
}