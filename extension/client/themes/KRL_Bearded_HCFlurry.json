{
    "name": "KRL Bearded  H C Flurry",
    "type": "dark",
    "colors": {
        "editor.background": "#1C1827",
        "editor.foreground": "#cdc7dd",
        "editorCursor.foreground": "#7fd7f5",
        "editorIndentGuide.activeBackground": "#554a75",
        "activityBar.background": "#171420",
        "activityBarBadge.background": "#7fd7f5",
        "activityBarBadge.foreground": "#1C1827",
        "sideBar.background": "#1C1827",
        "sideBar.border": "#0e0c14",
        "list.activeSelectionForeground": "#7fd7f5",
        "list.activeSelectionBackground": "#635345",
        "list.hoverBackground": "#1a1624",
        "statusBar.background": "#171420",
        "statusBar.foreground": "#cdc7dd",
        "tab.activeBorder": "#7fd7f5",
        "tab.activeBackground": "#1C1827",
        "titleBar.activeBackground": "#171420"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "punctuation.definition.comment.krl"
            ],
            "settings": {
                "foreground": "#554a75",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.fold.krl",
                "comment.line.fold.krl"
            ],
            "settings": {
                "foreground": "#2b1b58",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "keyword.function.movement.krl",
            "settings": {
                "foreground": "#7fd7f5",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.krl",
            "settings": {
                "foreground": "#bc98ff"
            }
        },
        {
            "scope": "storage.type.krl",
            "settings": {
                "foreground": "#F5DF76",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.variable.system.krl",
            "settings": {
                "foreground": "#fd604f",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#AFEA7B"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#ffaa7d"
            }
        },
        {
            "scope": "variable.language.data.krl",
            "settings": {
                "foreground": "#22D3B1"
            }
        },
        {
            "scope": "entity.name.function.krl",
            "settings": {
                "foreground": "#e4a3df"
            }
        }
    ]
}