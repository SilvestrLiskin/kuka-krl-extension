{
    "name": "KRL Bearded Oceanic Reverded",
    "type": "dark",
    "colors": {
        "editor.background": "#111c22",
        "editor.foreground": "#c3d6e0",
        "editorCursor.foreground": "#5fb2df",
        "editorIndentGuide.activeBackground": "#41687c",
        "activityBar.background": "#152229",
        "activityBarBadge.background": "#5fb2df",
        "activityBarBadge.foreground": "#111c22",
        "sideBar.background": "#111c22",
        "sideBar.border": "#0b1115",
        "list.activeSelectionForeground": "#5fb2df",
        "list.activeSelectionBackground": "#1e303a",
        "list.hoverBackground": "#131f26",
        "statusBar.background": "#152229",
        "statusBar.foreground": "#c3d6e0",
        "tab.activeBorder": "#5fb2df",
        "tab.activeBackground": "#111c22",
        "titleBar.activeBackground": "#152229"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "punctuation.definition.comment.krl"
            ],
            "settings": {
                "foreground": "#41687c",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.fold.krl",
                "comment.line.fold.krl"
            ],
            "settings": {
                "foreground": "#16435a",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "keyword.function.movement.krl",
            "settings": {
                "foreground": "#5fb2df",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.krl",
            "settings": {
                "foreground": "#978dd6"
            }
        },
        {
            "scope": "storage.type.krl",
            "settings": {
                "foreground": "#f6cc73",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.variable.system.krl",
            "settings": {
                "foreground": "#B4552D",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#97c892"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#DC8255"
            }
        },
        {
            "scope": "variable.language.data.krl",
            "settings": {
                "foreground": "#59c6c8"
            }
        },
        {
            "scope": "entity.name.function.krl",
            "settings": {
                "foreground": "#d194cd"
            }
        }
    ]
}