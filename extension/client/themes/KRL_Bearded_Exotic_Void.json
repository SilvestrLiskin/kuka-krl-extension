{
  "name": "KRL Bearded Exotic Void",
  "type": "dark",
  "colors": {
    "editor.background": "#171322",
    "editor.foreground": "#c7bfdb",
    "activityBar.background": "#120f1b",
    "sideBar.background": "#120f1b",
    "statusBar.background": "#171322",
    "editorLineNumber.foreground": "#4f4273"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#4f4273"
      }
    },
    {
      "scope": [
        "meta.fold.krl",
        "comment.line.fold.krl"
      ],
      "settings": {
        "foreground": "#655593",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "comment.line.todo.krl"
      ],
      "settings": {
        "foreground": "#3D8DE2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#6DBBFF"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#3D8DE2"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#C13838"
      }
    },
    {
      "scope": [
        "keyword.control.krl"
      ],
      "settings": {
        "foreground": "#A8547A"
      }
    },
    {
      "scope": [
        "keyword.function.movement.krl"
      ],
      "settings": {
        "foreground": "#7A63ED",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#c7bfdb"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#89C4FF"
      }
    }
  ]
}