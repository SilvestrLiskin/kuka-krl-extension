{
    "name": "KRL Bearded Monokai Black",
    "type": "dark",
    "colors": {
        "editor.background": "#141414",
        "editor.foreground": "#c6c6c6",
        "editorCursor.foreground": "#78dce8",
        "editorIndentGuide.activeBackground": "#545454",
        "activityBar.background": "#0e0e0e",
        "activityBarBadge.background": "#78dce8",
        "activityBarBadge.foreground": "#141414",
        "sideBar.background": "#141414",
        "sideBar.border": "#050505",
        "list.activeSelectionForeground": "#78dce8",
        "list.activeSelectionBackground": "#212121",
        "list.hoverBackground": "#111111",
        "statusBar.background": "#0e0e0e",
        "statusBar.foreground": "#c6c6c6",
        "tab.activeBorder": "#78dce8",
        "tab.activeBackground": "#141414",
        "titleBar.activeBackground": "#0e0e0e"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "punctuation.definition.comment.krl"
            ],
            "settings": {
                "foreground": "#545454",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.fold.krl",
                "comment.line.fold.krl"
            ],
            "settings": {
                "foreground": "#3b2020",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "keyword.function.movement.krl",
            "settings": {
                "foreground": "#78dce8",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.krl",
            "settings": {
                "foreground": "#ab9df2"
            }
        },
        {
            "scope": "storage.type.krl",
            "settings": {
                "foreground": "#ffd866",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.variable.system.krl",
            "settings": {
                "foreground": "#fc6a67",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#a9dc76"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#fc9867"
            }
        },
        {
            "scope": "variable.language.data.krl",
            "settings": {
                "foreground": "#78e8c6"
            }
        },
        {
            "scope": "entity.name.function.krl",
            "settings": {
                "foreground": "#e991e3"
            }
        }
    ]
}