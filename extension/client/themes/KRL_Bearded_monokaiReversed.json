{
    "name": "KRL Bearded Monokai Reversed",
    "type": "dark",
    "colors": {
        "editor.background": "#12141a",
        "editor.foreground": "#c6c9d7",
        "editorCursor.foreground": "#78dce8",
        "editorIndentGuide.activeBackground": "#4c536b",
        "activityBar.background": "#171921",
        "activityBarBadge.background": "#78dce8",
        "activityBarBadge.foreground": "#12141a",
        "sideBar.background": "#12141a",
        "sideBar.border": "#0a0b0f",
        "list.activeSelectionForeground": "#78dce8",
        "list.activeSelectionBackground": "#212430",
        "list.hoverBackground": "#15171e",
        "statusBar.background": "#171921",
        "statusBar.foreground": "#c6c9d7",
        "tab.activeBorder": "#78dce8",
        "tab.activeBackground": "#12141a",
        "titleBar.activeBackground": "#171921"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "punctuation.definition.comment.krl"
            ],
            "settings": {
                "foreground": "#4c536b",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.fold.krl",
                "comment.line.fold.krl"
            ],
            "settings": {
                "foreground": "#1d284e",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "keyword.function.movement.krl",
            "settings": {
                "foreground": "#78dce8",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.krl",
            "settings": {
                "foreground": "#ab9df2"
            }
        },
        {
            "scope": "storage.type.krl",
            "settings": {
                "foreground": "#ffd866",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.variable.system.krl",
            "settings": {
                "foreground": "#fc6a67",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#a9dc76"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#fc9867"
            }
        },
        {
            "scope": "variable.language.data.krl",
            "settings": {
                "foreground": "#78e8c6"
            }
        },
        {
            "scope": "entity.name.function.krl",
            "settings": {
                "foreground": "#e991e3"
            }
        }
    ]
}