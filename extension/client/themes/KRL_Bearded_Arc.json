{
  "name": "KRL Bearded arc",
  "type": "dark",
  "colors": {
    "editor.background": "#1c2433",
    "editor.foreground": "#afbbd2",
    "activityBar.background": "#181f2c",
    "sideBar.background": "#181f2c",
    "statusBar.background": "#1c2433",
    "editorLineNumber.foreground": "#d0d7e4"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#69717f"
      }
    },
    {
      "scope": [
        "meta.fold.krl",
        "comment.line.fold.krl"
      ],
      "settings": {
        "foreground": "#9fa5af",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "comment.line.todo.krl"
      ],
      "settings": {
        "foreground": "#FF955C",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#3CEC85"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#FF955C"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#E35535"
      }
    },
    {
      "scope": [
        "keyword.control.krl"
      ],
      "settings": {
        "foreground": "#F38CEC"
      }
    },
    {
      "scope": [
        "keyword.function.movement.krl"
      ],
      "settings": {
        "foreground": "#69C3FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#afbbd2"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#22ECDB"
      }
    },
    {
      "scope": [
        "variable.language.system.krl"
      ],
      "settings": {
        "foreground": "#EACD61",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "support.function"
      ],
      "settings": {
        "foreground": "#69C3FF"
      }
    },
    {
      "scope": [
        "punctuation.krl"
      ],
      "settings": {
        "foreground": "#afbbd2"
      }
    },
    {
      "scope": [
        "comment punctuation"
      ],
      "settings": {
        "foreground": "#69717f"
      }
    }
  ]
}