      {
        "id": "krl-icons",
        "label": "KRL File Icons",
        "path": "./client/krl-icon-theme.json"
      }
    ],
    "commands": [
      {
        "command": "krl.validateWorkspace",
        "title": "%command.validateWorkspace%",
        "category": "KRL",
        "icon": "$(check-all)"
      },
      {
        "command": "krl.foldAll",
        "title": "%command.foldAll%",
        "category": "KRL",
        "icon": "$(fold)"
      },
      {
        "command": "krl.unfoldAll",
        "title": "%command.unfoldAll%",
        "category": "KRL",
        "icon": "$(unfold)"
      },
      {
        "command": "krl.formatDocument",
        "title": "%command.formatDocument%",
        "category": "KRL",
        "icon": "$(code)"
      },
      {
        "command": "krl.removeTrailingWhitespace",
        "title": "%command.removeTrailingWhitespace%",
        "category": "KRL",
        "icon": "$(whitespace)"
      },
      {
        "command": "krl.sortDeclarations",
        "title": "%command.sortDeclarations%",
        "category": "KRL",
        "icon": "$(list-ordered)"
      },
      {
        "command": "krl.insertFold",
        "title": "%command.insertFold%",
        "category": "KRL",
        "icon": "$(symbol-namespace)"
      },
      {
        "command": "krl.refreshIOView",
        "title": "%command.refreshIOView%",
        "category": "KRL",
        "icon": "$(refresh)"
      },
      {
        "command": "krl.cleanupUnusedVariables",
        "title": "Clean Up Unused Variables",
        "category": "KRL",
        "icon": "$(trash)"
      },
      {
        "command": "krl.showCalculator",
        "title": "KRL Frame Calculator",
        "category": "KRL",
        "icon": "$(calculator)"
      },
      {
        "command": "krl.renameSignal",
        "title": "Rename Signal (Set Alias)",
        "category": "KRL",
        "icon": "$(edit)"
      },
      {
        "command": "krl.openSnippetGenerator",
        "title": "KRL Snippet Generator",
        "category": "KRL",
        "icon": "$(beaker)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "krlIO",
          "name": "KRL I/O Signals",
          "when": "workbenchState != empty"
        },
        {
          "id": "krlCommands",
          "name": "KUKA Commands",
          "when": "workbenchState != empty"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "krlIO",
        "contents": "%view.krlIO.welcome%\n[Refresh](command:krl.refreshIOView)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "krl.foldAll",
          "group": "navigation@1",
          "when": "resourceLangId == krl"
        },
        {
          "command": "krl.unfoldAll",
          "group": "navigation@2",
          "when": "resourceLangId == krl"
        },
        {
          "command": "krl.formatDocument",
          "group": "navigation@3",
          "when": "resourceLangId == krl"
        },
        {
          "command": "krl.validateWorkspace",
          "group": "navigation@4",
          "when": "resourceLangId == krl"
        }
      ],
      "editor/context": [
        {
          "command": "krl.validateWorkspace",
          "group": "1_modification@1",
          "when": "resourceLangId == krl"
        },
        {
          "command": "krl.formatDocument",
          "group": "1_modification@2",
          "when": "resourceLangId == krl"
        },
        {
          "command": "krl.removeTrailingWhitespace",
          "group": "1_modification@3",
          "when": "resourceLangId == krl"
        },
        {
          "command": "krl.sortDeclarations",
          "group": "1_modification@4",
          "when": "resourceLangId == krl"
        },
        {
          "command": "krl.insertFold",
          "group": "1_modification@5",
          "when": "resourceLangId == krl"
        },
        {
          "command": "krl.cleanupUnusedVariables",
          "group": "1_modification@6",
          "when": "resourceLangId == krl"
        }
      ]
    },
    "configuration": {
      "title": "%config.title%",
      "properties": {
        "krl.autoFold": {
          "type": "boolean",
          "default": false,
          "description": "%config.autoFold.description%"
        },
        "krl.formatOnSave": {
          "type": "boolean",
          "default": false,
          "description": "%config.formatOnSave.description%"
        },
        "krl.removeTrailingWhitespaceOnFormat": {
          "type": "boolean",
          "default": true,
          "description": "%config.removeTrailingWhitespaceOnFormat.description%"
        },
        "krl.indentWidth": {
          "type": "integer",
          "default": 3,
          "minimum": 1,
          "maximum": 8,
          "description": "%config.indentWidth.description%"
        },
        "krl.separateBeforeBlocks": {
          "type": "boolean",
          "default": false,
          "description": "%config.separateBeforeBlocks.description%"
        },
        "krl.separateAfterBlocks": {
          "type": "boolean",
          "default": false,
          "description": "%config.separateAfterBlocks.description%"
        },
        "krl.validateNonAscii": {
          "type": "boolean",
          "default": true,
          "description": "%config.validateNonAscii.description%"
        },
        "krl.errorLens.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Show diagnostic messages inline at the end of lines"
        },
        "krl.indentFolds": {
          "type": "boolean",
          "default": true,
          "description": "Indent content inside ;FOLD blocks"
        },
        "krl.alignAssignments": {
          "type": "boolean",
          "default": true,
          "description": "Align assignment operators (=) in consecutive lines"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -b client server",
    "watch": "tsc -b client server -w",
    "lint": "eslint . --ext .ts",
    "test": "node tests/extension.test.js"
  },
  "devDependencies": {
    "@types/node": "^18.19.130",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12",
    "vscode-uri": "^3.1.0"
  }
}
